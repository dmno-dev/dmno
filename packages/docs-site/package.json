{
  "name": "docs-site",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "netlify-build": "pnpm run docgen && pnpm run clidocgen && pnpm run build",
    "preview": "astro preview",
    "astro": "astro",
    "deploy": "pnpm build && netlify deploy --site=dmno --dir=dist --prod",
    "docgen": "pnpm exec typedoc --options ./src/utils/typedoc/typedoc.json",
    "clidocgen": "pnpm exec dmno get-cli-schema > ./src/utils/cli.json",
    "tapes": "node ./src/utils/tapes.js",
    "be-kind-rewind": "rm -rf ./src/assets/tapes/*",
    "lint": "eslint src --ext .ts,.cjs,.astro,.md,.mdx",
    "lint:fix": "pnpm run lint --fix"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.10",
    "@astrojs/starlight": "^0.21.5",
    "@astrojs/vue": "^4.0.11",
    "@astrojs/partytown": "^2.0.4",
    "@dmno/eslint-config": "workspace:*",
    "@dmno/tsconfig": "workspace:*",
    "@dmno/ui-lib": "workspace: *",
    "astro": "^4.5.18",
    "astro-iconify": "^1.2.0",
    "astro-robots-txt": "^1.0.0",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-mdx": "^3.1.5",
    "nanostores": "^0.10.0",
    "sharp": "^0.32.5",
    "starlight-blog": "^0.5.0",
    "starlight-links-validator": "^0.6.0",
    "typescript": "^5.3.3",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@dmno/1password-plugin": "../plugins/1password-plugin",
    "@dmno/core": "../core",
    "netlify-cli": "^17.1.0",
    "typedoc": "^0.25.12"
  }
}
