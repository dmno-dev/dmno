{
  "name": "@dmno/1password-plugin",
  "version": "1.0.0",
  "description": "adds ability to sync secrets with 1password to the DMNO config engine",
  "scripts": {
    "build": "tsup",
    "dev": "pnpm run build --watch",
    "lint": "eslint src --ext .ts,.cjs",
    "lint:fix": "pnpm run lint --fix",
    "postinstall": "node dist/install-cli.mjs"
  },
  "exports": {
    ".": {
      "default": "./dist/index.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "./dist/index.d.mts"
      ]
    }
  },
  "keywords": [],
  "author": "",
  "devDependencies": {
    "@dmno/eslint-config": "workspace:*",
    "@dmno/tsconfig": "workspace:*",
    "@types/async": "^3.2.24",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.30",
    "@types/unzip-stream": "^0.3.4",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@dmno/core": "workspace:*",
    "async": "^3.2.5",
    "lodash-es": "^4.17.21",
    "unzip-stream": "^0.3.1"
  }
}
