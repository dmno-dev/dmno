{
  "name": "dmno",
  "version": "0.0.5",
  "description": "dmno is the best way to manage config / env vars in your entire stack",
  "author": "dmno-dev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dmno-dev/dmno.git",
    "directory": "packages/core"
  },
  "bugs": "https://github.com/dmno-dev/dmno/issues",
  "homepage": "https://dmno.dev",
  "keywords": [
    "dmno",
    "config",
    "dmno.dev",
    "env vars",
    "environment",
    "process.env",
    "configuration",
    "secrets"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup",
    "build:ifnodist": "[ -d \"./dist\" ] && echo 'dist exists' || pnpm build",
    "dev": "pnpm run build --watch",
    "lint": "eslint src --ext .ts,.cjs",
    "lint:fix": "pnpm run lint --fix",
    "test": "vitest"
  },
  "engines": {
    "node": ">=16"
  },
  "exports": {
    ".": {
      "default": "./dist/index.mjs"
    },
    "./cli-lib": {
      "default": "./dist/cli/plugin-cli-lib.mjs"
    },
    "./load": {
      "default": "./dist/app-init/auto-load-global.mjs"
    }
  },
  "files": [
    "/dist"
  ],
  "typesVersions": {
    "*": {
      "index": [
        "./dist/index.d.mts"
      ],
      "cli-lib": [
        "./dist/cli/plugin-cli-lib.d.mts"
      ]
    }
  },
  "bin": {
    "dmno": "./dist/cli/commander-cli.mjs"
  },
  "devDependencies": {
    "@dmno/encryption-lib": "workspace:*",
    "@dmno/eslint-config": "workspace:*",
    "@dmno/ts-lib": "workspace:*",
    "@dmno/tsconfig": "workspace:*",
    "@inquirer/type": "^1.3.0",
    "@types/debug": "^4.1.12",
    "@types/diff": "^5.2.1",
    "@types/gradient-string": "^1.1.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.7",
    "@types/node-ipc": "^9.2.3",
    "@types/validate-npm-package-name": "^4.0.2",
    "@types/which": "^3.0.3",
    "tsup": "^8.0.2",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@dagrejs/graphlib": "^2.2.2",
    "@inquirer/core": "^8.0.1",
    "@inquirer/prompts": "^5.0.1",
    "acorn": "^8.11.3",
    "acorn-typescript": "^1.4.13",
    "base64-arraybuffer": "^1.0.2",
    "boxen": "^7.1.1",
    "cli-table3": "^0.6.4",
    "commander": "^12.0.0",
    "debug": "^4.3.4",
    "diff": "^5.2.0",
    "dotenv": "^16.4.5",
    "esm-resolve": "^1.0.11",
    "execa": "^8.0.1",
    "fdir": "^6.1.1",
    "gradient-string": "^2.0.2",
    "jsonc-parser": "^3.2.1",
    "kleur": "^4.1.5",
    "lodash-es": "^4.17.21",
    "log-update": "^6.0.0",
    "mitt": "^3.0.1",
    "modern-async": "^2.0.0",
    "node-ipc": "npm:@achrinza/node-ipc@^10.1.10",
    "outdent": "^0.8.0",
    "picomatch": "^3.0.1",
    "read-yaml-file": "^2.1.0",
    "svgo": "^3.2.0",
    "typescript": "^5.4.5",
    "validate-npm-package-name": "^5.0.0",
    "vite": "^5.2.10",
    "vite-node": "^1.5.0",
    "which": "^4.0.0"
  }
}
